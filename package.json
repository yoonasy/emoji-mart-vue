{
  "name": "emoji-mart-vue3-fast",
  "version": "0.0.1",
  "description": "Customizable Slack-like emoji picker for Vue3, fast version",
  "module": "dist/emoji-mart.js",
  "main": "dist/emoji-mart.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:serebrov/emoji-mart-vue.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "vuejs",
    "emoji",
    "picker"
  ],
  "files": [
    "dist",
    "data",
    "css"
  ],
  "author": "Etienne Lemay",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/serebrov/emoji-mart-vue/issues"
  },
  "homepage": "https://github.com/serebrov/emoji-mart-vue",
  "dependencies": {
    "@babel/polyfill": "7.2.5",
    "@babel/runtime": "7.3.4",
    "vue3-virtual-scroller": "^0.1.1"
  },
  "peerDependencies": {
    "vue": "^3.0.4"
  },
  "devDependencies": {
    "sass": "^1.28.0",
    "sass-loader": "^9.0.3",
    "@babel/core": "7.3.4",
    "@babel/plugin-transform-runtime": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.4",
    "@vue/test-utils": "^2.0.0-0",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "8.0.5",
    "css-loader": "^5.0.1",
    "emoji-datasource": "5.0.1",
    "emojilib": "^2.2.1",
    "inflection": "1.10.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "postcss": "^8.1.14",
    "postcss-loader": "^4.1.0",
    "prettier": "1.16.4",
    "size-limit": "^0.11.4",
    "typescript": "^3.9.7",
    "url-loader": "^0.5.8",
    "vue": "3.0.4",
    "vue-jest": "^5.0.0-0",
    "vue-loader": "^16.1.1",
    "vue-style-loader": "^4.1.2",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "scripts": {
    "clean": "rm -rf dist/",
    "build:data": "node scripts/build-data",
    "build:data-debug": "node --inspect=0.0.0.0:9230 --inspect-brk scripts/build-data",
    "build:dist": "webpack --config src/webpack.config.js",
    "build:dist-watch": "webpack -w --config src/webpack.config.js",
    "build:dist-stats": "webpack --config src/webpack.config.js --profile --json > stats.json",
    "build:docs": "webpack --config docs-src/webpack.config.js",
    "build": "npm run clean && npm run build:data && npm run build:dist",
    "dev:docs": "webpack --config src/webpack.config.js",
    "start": "npm run dev:docs",
    "stats": "webpack --config ./spec/webpack.config.js --json > spec/stats.json",
    "size-limit": "size-limit",
    "test": "NODE_ENV=test jest && size-limit",
    "jest": "jest --detectOpenHandles --forceExit --coverage",
    "jest-debug": "node --inspect=0.0.0.0:9230 --inspect-brk node_modules/.bin/jest --runInBand",
    "jest-clear": "jest --clearCache",
    "prettier-check": "prettier --check \"{src,spec}/**/*.js\"",
    "prettier": "prettier --write \"{src,spec}/**/*.js\""
  },
  "jest": {
    "testMatch": [
      "**/spec/*-spec.+(ts|tsx|js)"
    ],
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "transform": {
      ".+\\.vue$": "vue-jest",
      ".+\\.jsx?$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "moduleNameMapper": {
      ".+\\.css$": "<rootDir>/spec/css-stub.js",
      "../data/all.json$": "<rootDir>/data/all.json"
    },
    "transformIgnorePatterns": [
      "^.+\\.css$",
      "<rootDir>/node_modules/"
    ],
    "collectCoverageFrom": [
      "src/**",
      "!src/vendor/**",
      "!src/polyfills/**",
      "!src/webpack.config.js"
    ]
  },
  "size-limit": [
    {
      "path": "dist/emoji-mart.js",
      "limit": "80 KB"
    }
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "last 3 version",
    "IE >= 11",
    "iOS >= 9"
  ]
}
